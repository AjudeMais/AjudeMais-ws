	<artifactId>AjudeMais-domain</artifactId>
	<name>domain</name>

	<organization>
		<name>Ajude Mais</name>
	</organization>

	<developers>
		<developer>
			<name>Franck Arag√£o</name>
			<email>fjunior.aragao@gmail.com</email>
